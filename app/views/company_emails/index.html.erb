<h1>Listing company_emails</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @company_emails.each do |company_email| %>
  <tr>
    <td><%= company_email.name %></td>
    <td><%= company_email.email %></td>
    <td><%= link_to 'Show', company_email %></td>
    <td><%= link_to 'Edit', edit_company_email_path(company_email) %></td>
    <td><%= link_to 'Destroy', company_email, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

