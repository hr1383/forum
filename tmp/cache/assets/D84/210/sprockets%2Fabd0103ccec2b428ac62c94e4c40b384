        if (badwordsset.hasOwnProperty(descriptionValue[i])) {
          found = true;
          badwordsArray.push(descriptionValue[i]);
        }
        i++;
      }
      if (found) {
        alert("Text contains obsecene words." + badwordsArray);
        return false;
      }
      $("#divstep2").show();
      return false;
    });
